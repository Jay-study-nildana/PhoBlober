@{
}

@model PhotoAnalysedDTO

<h1>@ViewData["Title"]</h1>

<div>
    <p>Photo Analyse Successfull</p>

    <button><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="PhotoUploadForAnalysis">Analyse another photo</a></button>
</div>
<div>
    <ol>
    @foreach (var item in Model.imageTags)
    {
        <li>@item.Name : @item.Confidence</li>
    }
    </ol>
</div>
